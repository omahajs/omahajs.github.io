  "scripts": {
    "test": "grunt test",
    "test:ci": "npm test",
    "test:suite": "npm test && npm run nightwatch",
    "nightwatch": "nightwatch --config ./config/nightwatch.conf.js --env chrome",
    "nightwatch:headless": "nightwatch --config ./config/nightwatch.conf.js --env headless",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "rest-api": "json-server --port 8080 --watch test/data/db.json",
    "inspect": "grunt jsinspect:app",
    "build": "grunt build",
    "predemo": "npm run build",
    "demo": "grunt open:demo express:demo",
    "prestart": "nohup npm run rest-api &",
    "start": "grunt serve",
    "delete:remote": "git push origin --delete master && git fetch --all --prune",
    "delete:dist": "git rm -r --cached dist",
    "predelete": "npm run delete:remote",
    "delete": "npm run delete:dist",
    "predeploy": "npm run build && git add dist -f && git commit -m Deployment",
    "deploy": "git subtree push --prefix dist origin master"
  }
